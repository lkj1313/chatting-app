{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/route.ts"
    },
    {
      "src": "/api/login/(.*)",
      "dest": "/api/login/route.ts"
    },
    {
      "src": "/api/logout/(.*)",
      "dest": "/api/logout/route.ts"
    },
    {
      "src": "/api/userdataget/(.*)",
      "dest": "/api/userdataget/route.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
